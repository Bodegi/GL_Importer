<Window x:Class="GL_Importer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GL Importer" Height="156" Width="381">
    <Grid Margin="0,0,2,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="txtFile" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="Select File" VerticalAlignment="Top" Width="240" IsEnabled="False"/>
        <Button x:Name="btnBrowse" Content="Browse" HorizontalAlignment="Left" Height="23" Margin="255,10,0,0" VerticalAlignment="Top" Width="103" Click="btnBrowse_Click"/>
        <Button x:Name="btnTestData" Content="Test Data" HorizontalAlignment="Left" Height="47" Margin="10,60,0,0" VerticalAlignment="Top" Width="136" Click="btnTestData_Click"/>
    </Grid>
</Window>
